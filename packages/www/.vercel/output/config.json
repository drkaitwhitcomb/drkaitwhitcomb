{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/configs\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/keystatic(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/pages\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/keystatic(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/posts/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^(?:/(.*?))?/?$",
			"dest": "_render"
		}
	]
}